<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" styleClass="board" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rullofx.RulloController">
   <children>
      <StackPane fx:id="stackPane" styleClass="pane" VBox.vgrow="ALWAYS" />
      <VBox alignment="CENTER" prefHeight="0.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" styleClass="title" text="RulloFX" textAlignment="CENTER" />
            <HBox alignment="CENTER">
               <children>
                  <Button fx:id="nouveau" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="★" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="reset" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="⟲">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
